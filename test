curl -X POST http://127.0.0.1:5002/call-tts -H "Content-Type: application/json" -d "{\"text\":\"Hey meine liebe Deelaiar-Community, ich könnte vielleicht eure Stimme werden und hoffe, ihr habt bald Zeit, mir zu helfen, eine passende Stimme für mich zu finden, ganz viel Liebe an euch alle, ihr seid mega, eure Dilara.\",\"play_audio\":false,\"save_wav\":true,\"wav_path\":\"out\\only.wav\"}"

powershell -NoProfile -ExecutionPolicy Bypass -Command "$b=@{text='%tts_text%';play_audio=$false;save_wav=$true;wav_path='out\only.wav'} | ConvertTo-Json -Compress; Invoke-RestMethod -Method Post -Uri 'http://127.0.0.1:5002/call-tts' -ContentType 'application/json' -Body $b"


$b=@{text='%tts_text%';play_audio=$true;save_wav=$false;wav_path='out\only.wav'} | ConvertTo-Json -Compress; Invoke-RestMethod -Method Post -Uri 'http://127.0.0.1:5002/call-tts' -ContentType 'application/json' -Body $b

curl -X GET http://127.0.0.1:5002/ -H "Content-Type: application/json" 